{
  "expo": {
    "name": "github-explorer",
    "slug": "github-explorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gitsurfer.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gitsurfer.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.githubexplorer"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gitsurfer.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gitsurfer.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0239fb5f-c7dc-4fee-a008-fdacd3e3fabf"
      }
    },
    "owner": "vivek_react"
  }
}
